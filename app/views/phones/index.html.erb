<p id="notice"><%= notice %></p>

<h1>Phones</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Brand</th>
      <th>Manufacturer</th>
      <th>Chip type</th>
      <th>So version</th>
      <th>Screen type</th>
      <th>Display size</th>
      <th>Resolution</th>
      <th>Back cam</th>
      <th>Front cam</th>
      <th>Ram</th>
      <th>Processor</th>
      <th>Memory int</th>
      <th>Color</th>
      <th>Modaly</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Phone arquivos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phones.each do |phone| %>
      <tr>
        <td><%= phone.model %></td>
        <td><%= phone.brand %></td>
        <td><%= phone.manufacturer %></td>
        <td><%= phone.chip_type %></td>
        <td><%= phone.so_version %></td>
        <td><%= phone.screen_type %></td>
        <td><%= phone.display_size %></td>
        <td><%= phone.resolution %></td>
        <td><%= phone.back_cam %></td>
        <td><%= phone.front_cam %></td>
        <td><%= phone.ram %></td>
        <td><%= phone.processor %></td>
        <td><%= phone.memory_int %></td>
        <td><%= phone.color %></td>
        <td><%= phone.modaly %></td>
        <td><%= phone.quantity %></td>
        <td><%= phone.price %></td>
        <td><%= phone.phone_arquivos %></td>
        <td><%= link_to 'Show', phone %></td>
        <td><%= link_to 'Edit', edit_phone_path(phone) %></td>
        <td><%= link_to 'Destroy', phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone', new_phone_path %>
